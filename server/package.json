{
  "name": "surplus-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=staging nodemon --watch src --exec tsx src/server.ts",
    "start": "cross-env NODE_ENV=staging node dist/server.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "seed:items": "tsx scripts/seed-items.ts"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.8.7",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.1",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.11.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.4",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.14.10",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.4.5"
  }
}
